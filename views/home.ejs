<%- include('partials/header'); -%>

    <h1>Home</h1>
    <p>
        <%= homeStartingContent %>
    </p>
    <% POSTS.forEach(function(post){ %>
        <p>
        <h1>
            <%= post.title %>
        </h1>
        </p>
        <p>
            <%= post.content.substring(0, 100) + " ..." %>
                <a href="/posts/<%= post._id %> ">Read More</a>
        </p>

        <% }) %>


            <%- include('partials/footer'); -%>